{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "cd.. && husky install dashboard/.husky",
    "pre-commit-lint": "pnpm lint-staged",
    "test-coverage": "pnpm jest --coverage",
    "test": "pnpm jest",
    "pre-push-lint": "rm -rf storybook-static/ vite build"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.5",
    "next": "14.0.4",
    "react": "^18",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18",
    "react-error-boundary": "^4.0.11",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.48.2",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.5.6",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "extend-expect": "link:@testing-library/jest-dom/extend-expect",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "3.1.0",
    "sharp": "^0.32.6",
    "svgo": "^3.0.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --fix-dry-run --no-error-on-unmatched-pattern"
    ]
  }
}
